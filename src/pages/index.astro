---
import Layout from "@layouts/Layout.astro";
import type { Seo } from "@interfaces/Seo";
import Cabecera from "@components/inicio/Cabecera.astro";
import Formaciones from "@components/inicio/Formaciones.astro";
import Marquesina from "@components/extras/Marquesina.astro";
import Faq from "@components/extras/Faq.astro";
import Contacto from "@components/extras/Contacto.astro";
import Info from "@components/extras/Info.astro";

const seo: Seo = {
  title: "Cenakin OTEC Cursos Viña del Mar, Chile",
  description:
    "Un nuevo método de enseñanza para distintos cursos, con materiales incluidos, docentes kinesiólogos y calidad de servicio Viña del Mar, Chile.",
  url: "https://cenakin.cl",
  image: "https://cenakin.cl/images/logo_completo.jpg",
};
---

<Layout dataPag={seo}>
  <Cabecera />
  <Info />
  <Formaciones />
  <Marquesina
    texto="empoderamiento - profesionalismo - expansión - liderazgo -"
  />
  <Faq />
  <Contacto />
</Layout>
